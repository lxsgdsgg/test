<script>
  import {queryDailyReport,dailyReportSubmit,queryDailyReportExam} from "@/request/manage/report";

  export default {
    name: 'submitReport',
    data() {
      return {
        tableData6: [],
        form:{
          experience:'',
          agentName:'',
          nowadays:'',
          isSubmit:''
        },
        loadingView:false,
        rules:{
          experience: [
            { required: true, message: '必填项', trigger: 'change' }
          ],
          loadingView:false
        }
      };
    },
    methods:{
      _queryDailyReport(){
        this.loadingView = true
        let params = {}
        queryDailyReport(params).then(res =>{
          this.tableData6 = res
          this.loadingView = false
          for(let i in this.tableData6){
              this.form.experience = this.tableData6[i].experience
              this.form.agentName = this.tableData6[i].agentName
              this.form.nowadays = this.tableData6[i].nowadays
              this.form.isSubmit = this.tableData6[i].isSubmit
          }
        }).catch(error =>{
          this.loadingView = false
        })
      },
      //提交
      handleSubmit(form){
        this.$refs[form].validate(valid =>{
          if(this.tableData6[0].approval!=null){
            this.$message({
              showClose: true,
              type: 'warning',
              message: '您今日日报已审批，无法重复上报'
            })
            return false
          }
          else
              if(valid){
                for(let i in this.tableData6){
                  delete this.tableData6[i].nowadays
                  delete this.tableData6[i].agentName
                  delete this.tableData6[i].isSubmit
                  this.tableData6[i].experience = this.form.experience
                }
                dailyReportSubmit({jsonData:JSON.stringify( this.tableData6)}).then(res =>{
                    if(res.success){
                      // this.$refs['form'].resetFields();
                      // this._queryDailyReport()
                      this.$message({message:res.msg,type:'success'})
                    }else{
                      this.$message({message:res.msg,type:'warning'})
                    }
                }).catch(error=>{
                    console.log(error)
                })
              }
        })
      }
    },
    filters:{
      isSubmitFilter(value){
        if(value)
          return '已上报'
        else
          return '未上报'
      }
    }
    ,mounted(){
      this._queryDailyReport()
    }
  };
</script>
