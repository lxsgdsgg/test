router.beforeEach((to, from, next) => {
  NProgress.start()

  if (store.getters.isLock === 'lock' && to.path !== '/lock') {

    next({
      path: '/lock'
    })
    NProgress.done()

  } else if (to.path === '/lock' && store.getters.isLock !== 'lock') {
    next(from.fullPath)
    NProgress.done()
  }else {
    next()
  }
})

router.afterEach((to, from) => {
  let menuId = to.query['menu_id']
  let fromMenuId = from.query['menu_id']

  if (menuId === fromMenuId) {
    NProgress.done()
    return
  }

  store.dispatch('clearBtnPermission').then(() => {

    if (to.query && to.query['menu_id'] && (to.path.indexOf('redirect') === -1)) {
      // 获取该页面的权限按钮信息
      let menuId = to.query['menu_id']
      store.dispatch('getBtnPermission', menuId)
    }

  })

  NProgress.done()
})


export default router
