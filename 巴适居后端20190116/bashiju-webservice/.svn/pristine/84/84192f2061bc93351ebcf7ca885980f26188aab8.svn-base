		</if>
		<if test="spaceCodes!=null and spaceCodes.size()>0">
			and spaceQueryParam in
			<foreach collection="spaceCodes" item="item" separator="," open="(" close=")">
				CRC32(CONCAT(#{cityCode},#{item}))
			</foreach>
		</if>
		<if test="rooms!=null and rooms.size()>0">
