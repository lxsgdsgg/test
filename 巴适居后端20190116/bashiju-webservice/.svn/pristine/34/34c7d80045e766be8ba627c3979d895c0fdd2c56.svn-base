<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bashiju.webservice.mapper.NewHouseMapper">
	
	<select id="queryNewHouseDynamicByProjectId" resultType="" parameterType="string">
		select title,content,addTime from sys_community_project_news where projectId = #{projectId}
	</select>	
</mapper>