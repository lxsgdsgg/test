function saveCustomerInfo(data){
	
	$.ajax({
        type: "POST",//方法类型
        dataType: "json",//预期服务器返回的数据类型
        contentType: 'application/json',
        data:JSON.stringify(data),
        url: "<%=request.getContextPath()%>/customer/saveExchangeCustomerInfo" ,//url	                
        success: function (result) {
        	if(result.success){
        		alert("保存成功！");
        	}else{
        		alert(result.msg);
        	}
        },
        error : function() {
        	
        },
        beforeSend : function() {
        	
        }
    });  
} 

