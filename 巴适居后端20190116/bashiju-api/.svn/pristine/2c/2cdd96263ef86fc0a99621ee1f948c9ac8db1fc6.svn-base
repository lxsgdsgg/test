package com.bashiju.enums;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.TreeMap;

/**   
 * 账号类型
 * @ClassName:  AccountTypeEnum   
 * @Description:账号类型
