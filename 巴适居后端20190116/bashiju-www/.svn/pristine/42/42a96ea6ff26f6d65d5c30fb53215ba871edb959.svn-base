import java.util.List;

import com.bashiju.www.pojo.comm.HouseSortEnum;
import com.bashiju.www.pojo.service.out.comm.RangeResult;

public class HouseBaseParam {
	/**价格单位分**/
	protected List<RangeResult> prices;
	/**最低价格单位分**/
	protected Integer minPrice;
	/**最高价格单位分**/
	protected Integer maxPrice;
	/**面积单位平米**/
	protected List<RangeResult> spaces;
	/**最小面积单位平米**/
	protected Integer minSpace;
	/**最大面积单位平米**/
	protected Integer maxSpace;
	/**房间数**/
	protected List<Integer> rooms;
	/**几室**/
	protected Integer room;
	/**几厅**/
	protected Integer hall;
	/**几卫**/
	protected Integer toilet;
	/**楼层id**/
	protected Long houseTypeId;
	/**朝向id**/
	protected Long orientationId;
	/**装修id**/
	protected Long decorationId;
	/**楼龄**/
	protected String buildYears;
	/**房源用途编号**/
	protected String houseUse;
	/**排序**/
	private HouseSortEnum sort;
	public List<RangeResult> getPrices() {
		return prices;
	}
	public void setPrices(List<RangeResult> prices) {
		this.prices = prices;
	}
	public Integer getMinPrice() {
		return minPrice;
	}
	public void setMinPrice(Integer minPrice) {
		this.minPrice = minPrice;
	}
	public Integer getMaxPrice() {
		return maxPrice;
	}
	public void setMaxPrice(Integer maxPrice) {
		this.maxPrice = maxPrice;
	}
	public List<RangeResult> getSpaces() {
		return spaces;
	}
	public void setSpaces(List<RangeResult> spaces) {
		this.spaces = spaces;
	}
	public Integer getMinSpace() {
		return minSpace;
	}
	public void setMinSpace(Integer minSpace) {
		this.minSpace = minSpace;
	}
	public Integer getMaxSpace() {
		return maxSpace;
	}
	public void setMaxSpace(Integer maxSpace) {
		this.maxSpace = maxSpace;
	}
	public List<Integer> getRooms() {
		return rooms;
	}
	public void setRooms(List<Integer> rooms) {
		this.rooms = rooms;
	}
	public Integer getRoom() {
		return room;
	}
	public void setRoom(Integer room) {
		this.room = room;
	}
	public Integer getHall() {
		return hall;
	}
	public void setHall(Integer hall) {
		this.hall = hall;
	}
	public Integer getToilet() {
		return toilet;
	}
	public void setToilet(Integer toilet) {
		this.toilet = toilet;
	}
	public Long getHouseTypeId() {
		return houseTypeId;
	}
	public void setHouseTypeId(Long houseTypeId) {
		this.houseTypeId = houseTypeId;
	}
	public Long getOrientationId() {
		return orientationId;
	}
	public void setOrientationId(Long orientationId) {
		this.orientationId = orientationId;
	}
	public Long getDecorationId() {
		return decorationId;
	}
	public void setDecorationId(Long decorationId) {
		this.decorationId = decorationId;
	}
	public String getBuildYears() {
		return buildYears;
	}
	public void setBuildYears(String buildYears) {
		this.buildYears = buildYears;
	}
	public String getHouseUse() {
		return houseUse;
	}
	public void setHouseUse(String houseUse) {
		this.houseUse = houseUse;
	}
	public HouseSortEnum getSort() {
		return sort;
	}
	public void setSort(HouseSortEnum sort) {
		this.sort = sort;
	}
	@Override
	public String toString() {
		return "HouseBaseParam [prices=" + prices + ", minPrice=" + minPrice + ", maxPrice=" + maxPrice + ", spaces="
				+ spaces + ", minSpace=" + minSpace + ", maxSpace=" + maxSpace + ", rooms=" + rooms + ", room=" + room
				+ ", hall=" + hall + ", toilet=" + toilet + ", houseTypeId=" + houseTypeId + ", orientationId="
				+ orientationId + ", decorationId=" + decorationId + ", buildYears=" + buildYears + ", houseUse="
				+ houseUse + ", sort=" + sort.getDesc() + "]";
	}
	
	
}
